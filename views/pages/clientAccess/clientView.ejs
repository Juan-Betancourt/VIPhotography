<!DOCTYPE html>
<html>
    <%- include('../../partials/head') %>
<body>
    <%- include('../../partials/header') %>
    <%- include('../../partials/nav') %>
    <section id="image-container">
        <h5 id="wbh5">Welcome back <%=name%>!</h5>
        <div class="grid">
<% if(typeof img === 'object'){ %>
            <% img.forEach(pic => { %>

            <div class="module">
                <img src="<%= (pic.img_url || 'https://place-hold.it/200x200') %>" onclick="window.open('<%= pic.img_url %>')">
            </div>
        <% }); %>
<%} %>
    </section>
</body>
    <%- include('../../partials/footer') %>
</html>
